<p id="notice"><%= notice %></p>

<h1>Type Issues</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_issues.each do |type_issue| %>
      <tr>
        <td><%= link_to 'Show', type_issue %></td>
        <td><%= link_to 'Edit', edit_type_issue_path(type_issue) %></td>
        <td><%= link_to 'Destroy', type_issue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Issue', new_type_issue_path %>
